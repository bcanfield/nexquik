datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id    String @id
  name  String
  foo   Foo    @relation(fields: [fooId], references: [id], onDelete: Cascade)
  fooId String
}

model Foo {
  id      String    @id @default(cuid())
  bar     String
  Product Product[]
}
