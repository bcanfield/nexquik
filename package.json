{
  "name": "nexquik",
  "version": "1.2.0",
  "description": "Generate Next.js components from your Prisma Schema",
  "main": "dist/index.js",
  "repository": "https://github.com/bcanfield/nexquik",
  "bin": {
    "nexquik": "./dist/index.js"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:1": "npm run clean",
    "build:2": "npx tsc",
    "build:3": "npm run copy-files2",
    "start": "node dist/index.js",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 \"lib/**/*\" dist",
    "copy-files2": "copyfiles -u 1 \"templateApp/**/*\" dist/templateApp",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Brandin Canfield",
  "license": "MIT",
  "keywords": [
    "Prisma",
    "TypeScript",
    "Next.js",
    "React"
  ],
  "dependencies": {
    "@prisma/internals": "^4.14.0",
    "@types/jest": "^29.5.1",
    "@types/nunjucks": "^3.2.2",
    "@types/prettier": "^2.7.2",
    "chalk": "^4.1.2",
    "commander": "^10.0.1",
    "figlet": "^1.6.0",
    "prettier": "^2.8.8",
    "ts-toolbelt": "^9.6.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/figlet": "^1.5.6",
    "@types/node": "^20.1.2",
    "copyfiles": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  }
}
