// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Booking {
    id1          Int
    id2          Int
    inviteeEmail String
    startDateUTC DateTime
    cancelCode   String

    @@id([id1, id2])
}
