datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Product {
    id    String @id @map("_id")
    name  String
    foo   Foo    @relation(fields: [fooId], references: [id])
    fooId String
}

model Foo {
    id      String    @id @map("_id")
    bar     String
    Product Product[]
}
